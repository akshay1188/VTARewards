GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/KontaktSDK" $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/KontaktSDK" "${PODS_ROOT}/Headers/KontaktSDK"
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/KontaktSDK" "$(PODS_ROOT)/KontaktSDK" "$(PODS_ROOT)/KontaktSDK/KontaktSDK"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/KontaktSDK"
OTHER_LDFLAGS = $(inherited) -ObjC -l"kontakt-ios-sdk" -framework "CoreBluetooth" -framework "CoreLocation" -framework "Foundation" -framework "MobileCoreServices" -framework "SystemConfiguration" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods